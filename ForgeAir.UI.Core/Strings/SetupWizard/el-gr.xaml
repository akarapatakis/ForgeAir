<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:s="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Window-->
    <s:String x:Key="WindowTitle">Οδηγός Ρύθμισης ForgeAir</s:String>

    <!--Texts-->
    <s:String x:Key="WelcomeHeaderText">Καλωσορίσατε στο ForgeAir!</s:String>
    <s:String x:Key="WelcomeBodyText" >Αυτός ο οδηγός θα σας βοηθήσει να ρυθμίσετε το παρών λογισμικό για το ραδιοφωνικό σας σταθμό.</s:String>
    <s:String x:Key="WelcomeWarningMessage">Το ForgeAir βρίσκεται υπό ανάπτυξη. Παρακαλούμε, αναφέρετε τυχόν προβλήματα αμέσως στο: aggeloskarapatakis@outlook.com.gr</s:String>

    <!--Buttons-->
    <s:String x:Key="PreviousButton">Προηγούμενο</s:String>
    <s:String x:Key="NextButton">Επόμενο</s:String>
    <s:String x:Key="CancelButton">Άκυρο</s:String>

    <!--Step Texts-->
    <s:String x:Key="WelcomeStepText">Καλωσόρισμα</s:String>
    <s:String x:Key="DatabaseStepText">Βάση Δεδομένων</s:String>
    <s:String x:Key="MetadataStepText">Πληροφορίες Σταθμού</s:String>
    <s:String x:Key="AudioStepText">Ρύθμιση Ήχου</s:String>
    <s:String x:Key="InternetStepText">Ρύθμιση Διαδικτύου</s:String>
    <s:String x:Key="RDSStepText">Ρύθμιση RDS</s:String>
    <s:String x:Key="CompleteStepText">Ολοκλήρωση</s:String>
</ResourceDictionary>