<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:s="clr-namespace:System;assembly=mscorlib" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Window-->
    <s:String x:Key="WindowTitle">ForgeAir Setup Wizard</s:String>

    <!--Texts-->
    <s:String x:Key="WelcomeHeaderText">Welcome To ForgeAir!</s:String>
    <s:String x:Key="WelcomeBodyText">This wizard will help you setup ForgeAir for your radio station</s:String>
    <s:String x:Key="WelcomeWarningMessage">ForgeAir is under developement. Please report any bugs you encounter immediately to aggeloskarapatakis@outlook.com.gr</s:String>

    <!--Buttons-->
    <s:String x:Key="PreviousButton">Previous</s:String>
    <s:String x:Key="NextButton">Next</s:String>
    <s:String x:Key="CancelButton">Cancel</s:String>
    
    <!--Step Texts-->
    <s:String x:Key="WelcomeStepText">Welcome</s:String>
    <s:String x:Key="DatabaseStepText">Database Setup</s:String>
    <s:String x:Key="MetadataStepText">Station Metadata</s:String>
    <s:String x:Key="AudioStepText">Audio Setup</s:String>
    <s:String x:Key="InternetStepText">Internet Setup</s:String>
    <s:String x:Key="RDSStepText">RDS</s:String>
    <s:String x:Key="CompleteStepText">Complete</s:String>
</ResourceDictionary>